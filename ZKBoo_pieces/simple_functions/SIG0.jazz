fn ROTRIGHT_sig(reg u32 a, reg u8 b, reg u32 aux) -> stack u32 {

	aux = ((a >> b) | (a << (32 - b)));
	return aux;
}

fn SIG0(reg u32 x) -> reg u32 {

	reg u32 aux0, aux1, aux2;
	reg u8 b;


	b = 7;
	aux0 = ROTRIGHT_sig(x,b,aux0);

	b=18;
	aux1 = ROTRIGHT_sig(x,b,aux1);

	aux2 = x >> 3;

	x = aux0 ^ aux1 ^ aux2;

	return x;
} 

