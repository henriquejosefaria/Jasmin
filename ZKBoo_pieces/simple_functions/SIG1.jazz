fn ROTRIGHT_sig(reg u32 a, reg u8 b, reg u32 aux) -> stack u32 {

	aux = ((a >> b) | (a << (32 - b)));
	return aux;
}

fn SIG1(reg u32 x) -> reg u32{
	reg u32 aux, aux0, aux1, aux2;
	reg u8 b;

	b = 17;
	aux0 = ROTRIGHT_sig(x,b,aux0);
	
	b = 19;
	aux1 = ROTRIGHT_sig(x,b,aux1);

	aux2 = (x >> 10); 

	x = aux0 ^ aux1 ^ aux2;

	return x;
}

export 
fn test(){

	reg u32 x;

	
	x = SIG1(x);
}