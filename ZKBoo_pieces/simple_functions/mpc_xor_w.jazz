fn mpc_xor_w(stack u8[960] w, stack u8[12] y, stack u8[12] z, reg u8 pos_w) -> stack u8[12] {
	
	inline int i;

	for i=0 to 4{

		z[i] = w[(int)pos_w * 4 + i] ^ y[ i];
		z[4 + i] = w[320 + (int)pos_w * 4 + i] ^ y[4 + i];
		z[8 + i] = w[640 + (int)pos_w * 4 + i] ^ y[8 + i];

	}

	return z;
}

export 
fn test(){

	stack u8[12] y,z;
	stack u8[960] w;
	reg u8 pos_w;

	z = mpc_xor_w(w, y, z, pos_w);
}