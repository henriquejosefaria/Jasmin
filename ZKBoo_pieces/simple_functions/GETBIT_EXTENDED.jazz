
fn GETBIT_EXTENDED(stack u8[32] aux, stack u8 bit_idx) -> stack u8 {

	stack u8 selectedBit;
	stack u8 byte_idx;

	byte_idx =  bit_idx / 8;
	bit_idx = bit_idx - bit_idx / 8;

	selectedBit = aux[(int) byte_idx];
	selectedBit >>= bit_idx; 
	selectedBit &= 0x01;

	return  selectedBit;	
}




export 
fn test(){

	stack u8[32] aux;
	stack u8 res, bit_idx;

	res = GETBIT_EXTENDED(aux, bit_idx);
}